{
    "name": "ridislam/laravel-installer",
    "description": "food-express web installer",
    "type": "library",
    "require": {
        "php": "7.0.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rid Islam",
            "email": "i.ridislam@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RidIslam\\LaravelInstaller\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "RidIslam\\LaravelInstaller\\Providers\\LaravelInstallerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "master"
}
